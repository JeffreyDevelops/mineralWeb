<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="shortcut icon" href="/images/favicon.ico" type="image/x-icon">
    <title><%=parameter%> | Mineral</title>
</head>
<body>


    
  
    <%const array = push_player%>
    <%let name = parameter%>
    <% const result = array.find(element => { %>

        <% return element.toLowerCase() === name.toLowerCase() %>
        
    <% }); %>
    <% console.log(result) %>
    <%let UUID = array.indexOf(result);%>
    <%let UUID_player = array[UUID + 1];%>
    <% if (array.includes(result) === true) { %>
        
        <h1><%=result%></h1>

        <img src="https://visage.surgeplay.com/full/512/<%=UUID_player%>" alt="">

        <% } else { %>
            
            <h1>Something went wrong...</h1>

            <% } %>
</body>
</html>