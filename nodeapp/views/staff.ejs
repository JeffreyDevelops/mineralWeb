                <a style="text-align: center; text-decoration: none; margin-top: 1.5rem;" href="/player/<%=st_data.PLAYER%>"><span class="player_names" style="text-align: center; color: yellow; font-weight: bold; font-size: 1.5rem;"><%=st_data.PLAYER%></span></a>
              </div>
           <% }) %>
            </div>
          </div>
        </div>
      </div>
      <hr style="width: 100%; height: 5px; background-color: black; font-weight: bold; opacity: 1; position: relative; top: 1rem;">

