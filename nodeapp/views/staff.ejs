                  <a style="text-align: center; text-decoration: none; margin-top: 1.5rem;" href="/player/<%=result_s_hr_name_array[hr_name]%>"><img class="skin-size" src="https://visage.surgeplay.com/full/300/<%=result_s_hr_uuid_array[hr_uuid]%>" alt=""></a>
                  <a style="text-align: center; text-decoration: none; margin-top: 1.5rem;" href="/player/<%=result_s_hr_name_array[hr_name]%>"><span class="player_names" style="text-align: center; color: gray; font-weight: bold; font-size: 1.5rem;"><%=result_s_hr_name_array[hr_name]%></span></a>
              </div>
           <% } %>
            </div>
          </div>
        </div>
      </div>
      <hr style="width: 100%; height: 5px; background-color: black; font-weight: bold; opacity: 1; position: relative; top: 1rem;">


      <!-- Admin -->
      <div class="container">
        <div class="row">
          <div style="padding: 46px;" class="col">
            <div>
              <h1 class="staff-title" style="text-align: center; margin-bottom: 6rem; color: rgba(255, 255, 255, 0.795); font-size: 4rem; font-weight: bold; text-decoration: underline; text-underline-offset: 13px;"><span style="color: #ff6565;">Admin</span></h1>
            </div>
            <div style="gap: 5rem;" class="d-flex justify-content-center align-items-center flex-wrap">
              <% smUser.forEach(function(sm_data) { %>
